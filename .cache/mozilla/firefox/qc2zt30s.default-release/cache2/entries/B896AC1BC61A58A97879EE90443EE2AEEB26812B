//tdna ê³¼ëª© ì ìš©
function getTdnaSelect(kjkey, brd_id, id, seq, mdv, fdv, my){
  $.ajax({
    url: "/ilos/co/tdna_select_form.acl",
    type: "POST",
    data: { 
      TDNA_KJKEY  : kjkey,
      TDNA_BRD_ID : brd_id,
      TDNA_SEQ    : seq,
      mdv         : mdv,
      fdv         : fdv,
      my          : my,
      encoding    : "utf-8"
    },
    async: false,
    success: function(data){
      if(data.isError){
        alert(data.message);
      }else{
        $("#"+id).append(data);
        $("#"+id).show();
      }
    },
    error : ajaxErrorMsg
  });
}

