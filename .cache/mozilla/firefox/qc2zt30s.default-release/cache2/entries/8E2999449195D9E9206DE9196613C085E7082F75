function showLayout(menuId) {
  topForm.MENU_ID.value = menuId;
  topForm.action_id.value = "/cmm/layout/site_layout";
  topForm.method = "post";
  topForm.action = "action.do";
  topForm.submit();
}

/**
 1.menuIdë¥¼ ì´ìš©í•´ ì–´ë– í•œ ìƒìœ„ ë©”ë‰´ ì•„ì´ë””ê°€ í´ë¦­ë˜ì—ˆëŠ”ì§€ ì„œë²„ì— ë„˜ê²¨ì¤€ë‹¤.
      ì„œë²„ì—ì„œ ë©”ë‰´ ì•„ì´ë””ë¥¼ ì´ìš©í•´ í•´ë‹¹ í™”ë©´ì˜ ë ˆì´ì•„ì›ƒ ì •ë³´ë¥¼ ì´ìš©í•˜ì—¬ í™”ë©´ì— ë¿Œë ¤ì¤€ë‹¤.
 2.í™”ë©´ ë ˆì´ì•„ì›ƒì—ì„œ ì»¨í…ì¸  ë©”ë‰´ê°€ í´ë¦­ëœ íš¨ê³¼ë¥¼ ì–»ê¸° ìœ„í•´ì„œ contentsActionIdì™€ paramsê°’ì„ ì´ìš©í•´ ì„œë²„ì— ì¬ìš”ì²­ì„ í•œë‹¤.
   (/laycontents.js ì°¸ê³ ) 
